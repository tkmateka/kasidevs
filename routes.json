                },
                {
                    "component": "servicesComponent",
                    "path": "services"
                },
                {
                    "component": "getinvolvedComponent",
                    "path": "getinvolved"
                },
                {
                    "component": "recruitmentComponent",
                    "path": "recruitment"
                },
                {
                    "component": "beeComponent",
                    "path": "bee"
                },
                {
                    "component": "gallaryComponent",
                    "path": "gallary"
                },
                {
                    "component": "newsComponent",
                    "path": "news"
                },
                {
                    "component": "contactComponent",
                    "path": "contact"
                }
